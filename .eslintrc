{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "indent": ["error", 1], 
      "no-unused-vars": "warn", 
      "no-console": "off", 
      "quotes": ["error", "single"],
      "semi": ["error", "always"]
    }
  }